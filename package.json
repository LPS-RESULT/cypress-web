{
  "private": true,
  "name": "cypress-example-todomvc",
  "version": "0.0.0-development",
  "description": "This repo contains an example React App, with the tests written in Cypress",
  "scripts": {
    "build": "babel --presets env js/app.js -o build/app.bundle.js",
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record --key 4e7e8041-7a00-461c-96e6-4af290f8d022",
    "cypress:run:ui:record": "cypress run --record --key 4e7e8041-7a00-461c-96e6-4af290f8d022 --group 'happyfeet-ui' --spec 'cypress/integration/ui/**/*'",
    "cypress:run:api:record": "cypress run --record --key 4e7e8041-7a00-461c-96e6-4af290f8d022 --group 'happyfeet-api' --spec 'cypress/integration/api/**/*'",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "dev": "run-p --race start:ci cypress:open",
    "start": "node app",
    "start:ci": "node app",
    "test": "npm run start & cypress run",
    "test:ci": "run-p --race start:ci cypress:run",
    "test:ci:record": "run-p --race start:ci cypress:run:record",
    "test:ci:chrome": "run-p --race start:ci cypress:run:chrome",
    "test:ci:headed": "run-p --race start:ci cypress:run:headed",
    "lint": "eslint --fix cypress/*/*.js",
    "effective:circle:config": "circleci config process circle.yml | sed /^#/d"
  },
  "dependencies": {
    "base-64": "0.1.0",
    "body-parser": "1.18.3",
    "director": "1.2.8",
    "express": "4.16.4",
    "express-basic-auth": "1.1.6",
    "json-middleware": "1.0.2",
    "react": "0.13.3",
    "todomvc-app-css": "2.1.2",
    "todomvc-common": "cypress-io/todomvc-common#88b7c6359ad4a5097312d8b2a21dd539ce9f4446"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "cypress": "3.1.4",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-cypress-dev": "2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-mocha": "5.2.1",
    "eslint-plugin-react": "^7.8.2",
    "mocha-allure-reporter": "1.4.0",
    "npm-run-all": "4.1.5"
  }
}
